      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Properties\Icons\icons8_image_120px_3.png" />
    <Resource Include="Properties\Icons\minimizeIcon.png" />
    <Resource Include="Properties\Icons\minimizeInWindowIcon.png" />
    <Resource Include="Properties\Icons\trash_24px.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Properties\Icons\trash_24px_hovered.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Properties\Icons\trash_24px_pressed.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="6.0.1" />
  </ItemGroup>

</Project>
